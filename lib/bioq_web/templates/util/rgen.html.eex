<div class="col-md-3" id="inputPanel">
<%= form_for @conn, util_path(@conn, :rgen), [as: :rgen_params], fn f -> %>
  <label for="inputDistribution">Distribution</label>
  <%= select f, :distribution , ["Normal": "rnorm", "Uniform": "runif"] , class: "form-control"%><br>
  <label for="inputSize">Size</label>
  <%= text_input f, :size, class: "form-control"%><br>
  <%= submit "Generate", class: "btn btn-default" %>
<% end %>
</div>
<div class="col-md-9" id="outputPanel">
<%=  render @view_module, "rgen-output.html", assigns %>
</div>